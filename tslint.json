{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "**/*.json"
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "object-literal-key-quotes": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "no-console": false,
    "variable-name": false,
    "only-arrow-functions": [],
    "arrow-parens": ["always"]
  }
}